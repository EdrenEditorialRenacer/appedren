<div class="container p-4">
    <div class="row">
        {{#each documentos}}
            <div class="col-md-3">
                <div class="card text-center">
                    <div class="card-body">

                        <embed src="../{{documento}}#toolbar=0" type="application/pdf" width="100%" height="400px" controlsList="nodownload" />
                      
                         <p>{{idarchivero}}</p>  
                        
                        
                        
                       
                    </div>
                </div>
            </div>
        {{else}}
            <div class="col-md-4 mx-auto">
                <div class="card card-body text-center">
                    <p>No hay Documentos guadados aun.</p>
                    <a href="/documentos/add">Crea uno!</a>
                </div>
            </div>
        {{/each}}
    </div>
</div>
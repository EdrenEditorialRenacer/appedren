<table class="table">
    <thead>
        <tr>
            <th scope="col">#</th>
            <th scope="col">Asunto</th>
            <th scope="col">Comentario</th>
            <th scope="col">Usuario</th>
            <th scope="col">Telefono</th>
            <th scope="col">Email</th>


        </tr>
    </thead>
    <tbody>
        {{#each comentarios}}
        <tr>
            <td>{{id}}</td>
            <td>{{asunto}}</td>
            <td>{{username}}</td>
            <td>{{telefono}}</td>
            <td>{{email}}</td>
        </tr>
        {{else}}
        <h2>No hay Comentarios Registrados</h2>
        {{/each}}


    </tbody>
</table>
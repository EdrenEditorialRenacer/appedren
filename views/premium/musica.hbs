<div class="container p-4">
    <div class="row">
        {{#each musica}}
        <div class="col-md-3">
            <div class="card text-center">
                <div class="card-body">
                    <h2>{{titulo}}</h2>
                    <img src="../{{portada}}" alt="{{portada}}" width="200px" height="150px">
                    <h1>{{autor}}</h1>
                    <p>{{facebook}}</p>
                    <p>{{whattsap}}</p>
                    <p>{{email}}</p>
                    <audio id="sonido" src="../{{documento}}" controls controlsList="nodownload">
                    </audio>
                </div>
            </div>
        </div>
        {{else}}
        <div class="col-md-4 mx-auto">
            <div class="card card-body text-center">
                <p>No hay Musica aun en existencia.</p>

            </div>
        </div>
        {{/each}}
    </div>
</div>
<div class="container p-4">
    <div class="row">
        {{#each all}}
            <div class="col-md-3">
                <div class="card text-center">
                    <div class="card-body">
                        <h2>{{titulo}}</h2>
                        <img src="../{{portada}}" alt="{{portada}}" width="200px" height="150px">
                        <h1>{{autor}}</h1>
                    </div>
                </div>
            </div>
        {{else}}
            <div class="col-md-4 mx-auto">
                <div class="card card-body text-center">
                    <p>No hay Material aun en existencia.</p>

                </div>
            </div>
        {{/each}}
        <a href="/premium/libros" class="btn btn-warning">Ir a Libros</a>
        <hr>
        <a href="/premium/revistas" class="btn btn-warning">Ir a Revistas</a>
        <hr>
        <a href="/premium/comics" class="btn btn-warning">Ir a Comics</a>
        <hr>
        <a href="/premium/musica" class="btn btn-warning">Ir a Musica</a>
        <hr>
        <a href="/premium/peliculas" class="btn btn-warning">Ir a Peliculas</a>
        <hr>
        <a href="/premium/videos" class="btn btn-warning">Ir a Series y Videos</a>
        <hr>
    </div>
</div>